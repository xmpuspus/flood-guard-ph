{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/demo_ui",
      "dest": "/demo_ui/index.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/demo_ui/assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "backend/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
